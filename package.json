{
  "name": "@platformbuilders/react-ui",
  "version": "0.0.1",
  "description": "Platform Builders Shared Components Library For React Web",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/platformbuilders/react-ui.git"
  },
  "keywords": [
    "platformbuilders",
    "react",
    "react-ui"
  ],
  "author": "Platform Builders <dev@platformbuilders.io>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "clean": "rm -rf ./node_modules && yarn install",
    "pretty": "prettier --config ./.prettierrc.js --write \"src/**/*.{ts,tsx,js}\"",
    "lint": "eslint '*/**/*.{ts,tsx}' --fix",
    "build": "rm -rf dist && yarn lint && tsc",
    "prepublish": "yarn build",
    "precommit": "yarn lint",
    "preversion": "yarn lint",
    "postversion": "git push && git push --tags",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major",
    "storybook": "start-storybook -p 9009 -c storybook",
    "storybook:build": "rimraf storybook-static; build-storybook -c storybook"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn pretty",
      "yarn lint",
      "git add ."
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "peerDependencies": {
    "react": ">=16.11.0",
    "react-router-dom": ">=5",
    "styled-components": ">=5"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.14",
    "@platformbuilders/helpers": "^0.1.1",
    "formik": "^2.1.4",
    "lodash": "^4.17.15",
    "polished": "^3.6.0",
    "react": "^16.13.1",
    "react-input-mask": "^2.0.4",
    "react-lottie": "^1.2.3",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-backgrounds": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addon-viewport": "^5.3.19",
    "@storybook/addons": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "@types/lodash": "^4.14.150",
    "@types/moment": "^2.13.0",
    "@types/numeral": "^0.0.27",
    "@types/react": "^16.9.35",
    "@types/react-input-mask": "^2.0.5",
    "@types/react-lottie": "^1.2.5",
    "@types/react-router-dom": "^5.1.5",
    "@types/styled-components": "^5.1.0",
    "@types/yup": "^0.28.1",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.33.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-babel-module": "5.1.2",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.1",
    "eslint-plugin-json": "^2.1.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "eslint-plugin-sonarjs": "^0.5.0",
    "file-loader": "^6.0.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.1.6",
    "prettier": "^2.0.5",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-native": "^0.62.2",
    "react-native-web": "^0.12.3",
    "storybook-addon-paddings": "^2.0.2",
    "storybook-readme": "^5.0.8",
    "style-loader": "^1.2.1",
    "styled-components": "^5.0.1",
    "svg-inline-loader": "^0.8.2",
    "ts-loader": "^7.0.5",
    "typescript": "^3.8.3"
  }
}
