{
  "name": "@platformbuilders/react-elements",
  "version": "0.3.4",
  "private": false,
  "description": "Platform Builders Shared Components Library For React Web",
  "keywords": [
    "platformbuilders",
    "react",
    "elements"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/platformbuilders/react-elements.git"
