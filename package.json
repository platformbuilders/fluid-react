{
  "name": "@platformbuilders/react-ui",
  "version": "0.1.0",
  "description": "Platform Builders Shared Components Library For React Web",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/platformbuilders/react-ui.git"
  },
  "keywords": [
    "platformbuilders",
    "react",
    "react-ui"
  ],
  "author": "Platform Builders <dev@platformbuilders.io>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "clean": "rm -rf ./node_modules && yarn install",
    "pretty": "prettier --config ./.prettierrc.js --write \"src/**/*.{ts,tsx,js}\"",
    "lint": "tsc --noEmit && eslint '*/**/*.{ts,tsx}' --fix",
    "test": "jest --passWithNoTests",
    "test:watch": "yarn test -- --watch",
    "test:coverage": "yarn test -- --coverage",
    "build": "rm -rf dist && yarn lint && tsc",
    "prepublish": "yarn build",
    "precommit": "yarn lint",
    "preversion": "yarn lint",
    "postversion": "git push && git push --tags",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major",
    "storybook": "start-storybook -p 9009 -c storybook",
    "storybook:build": "rimraf storybook-static; build-storybook -c storybook"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn pretty",
      "yarn lint",
      "yarn test"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "peerDependencies": {
    "react": ">=16.11.0",
    "react-router-dom": ">=5",
    "styled-components": ">=5"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@platformbuilders/helpers": "^0.3.2",
    "formik": "^2.1.5",
    "lodash": "^4.17.20",
    "polished": "^3.6.6",
    "react": "^16.13.1",
    "react-input-mask": "^2.0.4",
    "react-lottie": "^1.2.3",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@storybook/addon-actions": "6.0.21",
    "@storybook/addon-backgrounds": "6.0.21",
    "@storybook/addon-knobs": "6.0.21",
    "@storybook/addon-links": "6.0.21",
    "@storybook/addon-viewport": "6.0.21",
    "@storybook/addons": "6.0.21",
    "@storybook/react": "^6.0.21",
    "@types/enzyme": "^3.10.6",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/faker": "^5.1.0",
    "@types/jest": "^26.0.14",
    "@types/lodash": "^4.14.161",
    "@types/moment": "^2.13.0",
    "@types/numeral": "^0.0.28",
    "@types/react": "^16.9.49",
    "@types/react-input-mask": "^2.0.5",
    "@types/react-lottie": "^1.2.5",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-test-renderer": "^16.9.3",
    "@types/styled-components": "^5.1.3",
    "@types/yup": "^0.29.7",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^4.3.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-typescript": "^10.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-babel-module": "5.1.2",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.1",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-sonarjs": "^0.5.0",
    "faker": "^5.1.0",
    "file-loader": "^6.1.0",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "jest-canvas-mock": "^2.2.0",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-test-renderer": "^16.13.1",
    "storybook-addon-paddings": "^3.0.0",
    "storybook-readme": "^5.0.8",
    "style-loader": "^1.2.1",
    "styled-components": "^5.2.0",
    "svg-inline-loader": "^0.8.2",
    "ts-jest": "^26.3.0",
    "ts-loader": "^8.0.3",
    "typescript": "^4.0.2"
  }
}
